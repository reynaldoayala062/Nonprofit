<%= stylesheet_link_tag 'user.css' %>
<h1> Edit Programmers Info </h1>
<%= form_for @programmer do |p| %>
<%= p.label :name %>
<%= p.text_field :name %>
<br><br>
<%= p.label :email %>
<%= p.text_field :email %>
<br><br>
<%= p.label :phone %>
<%= p.text_field :phone %>
<br><br>
<%= p.submit %>
<% end %>

